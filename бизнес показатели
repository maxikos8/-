   "source": [
    "xbox = df_filt\n",
    "xbox['user_score'].fillna(0,inplace=True)\n",
    "xbox = xbox.query('platform == \"xone\" and user_score !=0')\n",
    "xbox['user_score'].var()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 56,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "1.7657529001268804"
      ]
     },
     "execution_count": 56,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "pc = df_filt\n",
    "pc['user_score'].fillna(0,inplace=True)\n",
    "pc = pc.query('platform == \"pc\" and user_score !=0')\n",
    "pc['user_score'].var()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 57,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "p - значение 9.16488542337547e-06\n",
      "Отвергаем нулевую гипотезу\n"
     ]
    }
   ],
   "source": [
    "results = st.ttest_ind(xbox['user_score'],pc['user_score'],equal_var=False)\n",
    "print('p - значение' , results.pvalue)\n",
    "if results.pvalue < alpha:\n",
    "    print(\"Отвергаем нулевую гипотезу\")\n",
    "else:\n",
    "    print(\"Не получилось отвергнуть нулевую гипотезу\")"
   ]
