    "### Платформы с наибольшими суммарными продажами и построение распределение по годам\n",
    "<a id=\"year\"></a>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [],
   "source": [
    "#создадим таблицу по суммарным продажам по годам\n",
    "platform=df.pivot_table(index='platform',values='general_sales',columns='year_of_release',aggfunc='sum')\n",
    "platform.fillna(0,inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "platform['full'] = 0 #колонка для суммарных продаж по всем годам\n",
    "for j in platform.columns.unique():\n",
    "    if j != 'full':\n",
    "        platform['full'] += platform[j]\n",
    "platform = platform.sort_values(by='full',ascending=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": []
